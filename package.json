{
  "name": "Pigalle by Villamoura Restaurant",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "prebuild": "mkdir -p .11ty-vite && rimraf _site",
    "build:vercel": "pnpm run prebuild && npx eleventy --config=.eleventy-build.js",
    "vercel-build": "pnpm run build:vercel",
    "build:win": "powershell -ExecutionPolicy Bypass -File build.ps1",
    "dev": "eleventy --config=.eleventy.js --serve --incremental",
    "clean": "rimraf _site .11ty-vite"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-plugin-vite": "^4.0.0",
    "@prismicio/client": "^7.4.1",
    "@types/events": "^3.0.3",
    "@types/fontfaceobserver": "^2.1.3",
    "@types/html-minifier": "^4.0.5",
    "@types/lodash": "^4.17.1",
    "@types/normalize-wheel": "^1.0.4",
    "@types/ua-parser-js": "^0.7.39",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "html-minifier": "^4.0.0",
    "lil-gui": "^0.18.2",
    "prettier": "^3.2.5",
    "pug": "^3.0.3",
    "rimraf": "^6.0.1",
    "sass": "^1.76.0",
    "stats.js": "^0.17.0",
    "typescript": "^5.4.5",
    "vite-plugin-pwa": "^1.0.0"
  },
  "dependencies": {
    "@11ty/eleventy-plugin-pug": "^1.0.0",
    "@imgix/js-core": "^3.8.0",
    "auto-bind": "^5.0.1",
    "axios": "^1.11.0",
    "curtainsjs": "^8.1.6",
    "events": "^3.3.0",
    "fontfaceobserver": "^2.3.0",
    "gsap": "^3.13.0",
    "include-media": "^2.0.0",
    "lenis": "^1.3.9",
    "lodash": "^4.17.21",
    "nodelist-foreach-polyfill": "^1.2.0",
    "normalize-wheel": "^1.0.1",
    "prefix": "^1.0.0",
    "ua-parser-js": "^1.0.37",
    "vite": "^6.3.1",
    "vite-plugin-clean": "^2.0.1"
  }
}